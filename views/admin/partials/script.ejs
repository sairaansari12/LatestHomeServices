    <script>
      
    $("#countryCode").intlTelInput({
        initialCountry: "in",
    });
    var input = document.querySelector("#countryCode");
    input.addEventListener("countrychange", function () {
        $("#countryCode").val($("#countryCode").intlTelInput("getSelectedCountryData").dialCode);

    });

    </script>
  <script>
    var images = ["bg-auth.png", "bg-auth1.png","slide 1.png","slide 2.png","slide 3.png","slide 4.png","slide 5.png","slide 6.png"];
    $(function () {
        var i = 0;
        $(".auth-fluid-right").css("background", "url(/dist/assets/images/" + images[i] + ")");
        setInterval(function () {
            i++;
            if (i == images.length) {
                i = 0;
            }
            $(".auth-fluid-right").fadeOut("slow", function () {
               // console.log("/dist/assets/images/" + images[i])
                $(this).css("background", "url('/dist/assets/images/" + images[i] + "')", "!important");
                $(this).css("background-repeat", "no-repeat !important");


              
                $(this).fadeIn("slow");
            });
        }, 4000);
    });
 </script>